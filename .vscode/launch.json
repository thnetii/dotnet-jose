  "version": "0.2.0",
  "configurations": [
    {
      "name": "THNETII.Security.JOSE.Cli",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/bin/Debug/netcoreapp2.1/THNETII.Security.JOSE.Cli.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/THNETII.Security.JOSE.Cli",
      "console": "internalConsole",
      "stopAtEntry": false,
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:pickProcess}"
    }
  ]
}
